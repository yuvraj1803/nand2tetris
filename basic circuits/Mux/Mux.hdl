/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel,out=selnot);
    And(a=a,b=selnot,out=aselnot);
    And(a=b,b=sel,out=bsel);
    Or(a=aselnot,b=bsel,out=out);
}
